    <script>
        if (typeof onGridReady !== "undefined")
            onGridReady();

        $('.super-autocomplete').each(function (index, item) {
            var $item = $(item);
            var url = $item.attr('dataAutoCompleteUrl');
            var parentId = $item.attr('dataParentId');
            var dataParentIdFromElementId = $item.attr('dataParentIdFromElementId');
            var name = $item.attr('data-grid-filter-name');
            //sholkany
            var field = $item.attr('data-grid-filter-field');
            //sholkany
            var options = {
                valueField: 'Id',
                labelField: 'Text',
                searchField: 'Text',
                options: [],
                create: false,
                render: {
                    option: function (item, escape) {
                        return '<div>' + escape(item.Text) + '</div>';
                    }
                },
                onChange: function (value) {

                },
                load: function (query, callback) {
                    if (dataParentIdFromElementId) {
                        debugger;
                        var pid = $("#" + dataParentIdFromElementId).val();
                        parentId = pid;
                    }

                    if (!query.length) return callback();
                    $.ajax({
                        url: url,
                        type: 'GET',
                        dataType: 'json',
                        data: {
                            text: query,
                            parentId: parentId,
                            //sholkany
                            field: field
                        },
                        error: function () {
                            callback();
                        },
                        success: function (res) {
                            callback(res.Data);
                        }
                    });
                },
                onInitialize: function () {
                    var ids = $item.attr('dataPreSelectedIds');
                    var field = $item.attr('data-grid-filter-field');
                    if (!ids)
                        return;

                    $.ajax({
                        url: url,
                        type: 'GET',
                        dataType: 'json',
                        data: {
                            text: ids,
                            isIdList: true,
                            parentId: parentId,
                            field: field
                        },
                        success: function (data) {
                            item.selectize.addOption(data.Data); // This is will add to option
                            var selected_items = [];
                            $.each(data.Data, function (i, obj) {
                                selected_items.push(obj.Id);
                            });
                            item.selectize.setValue(selected_items, true); //this will set option values as default
                        }
                    });
                }
            };
            var maxItems = parseInt($item.attr("dataMaxItems"));
            if (!isNaN(maxItems))
                options.maxItems = maxItems;

            $item.selectize(options);
        });

        $(document).ready(function () {
            $('#example').DataTable({
                "paging": true,
                "pageLength": 15,
                "scrollX": true,
