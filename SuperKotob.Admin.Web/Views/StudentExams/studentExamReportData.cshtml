@model IEnumerable<Tollab.Admin.Data.Models.Views.StudentExamView>

@{
    ViewBag.Title = "studentExamReportData";
}
 

<head>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.10.22/css/dataTables.bootstrap.min.css" />
</head>


@using (Html.BeginForm())
{

    <input type="submit" value="Get Excel File" class="btn btn-primary" formaction="/StudentExams/GetExcelFile?ExamId=@ViewBag.ExamId&StudentId=@ViewBag.StudentId" style="margin-bottom:10px;" />


    <table id="example" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ParentName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ParentName2)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ParentPhone)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ParentPhone2)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ExamName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TotalScore)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TotalDegree)
                </th>
            </tr>

        </thead>
        <tbody>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ParentName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ParentName2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ParentPhone)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ParentPhone2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ExamName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TotalScore)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TotalDegree)
                    </td>
                    
                </tr>
            }

        </tbody>
    </table>
}
<a href="#" onclick="goBack()" class="btn bg-green-active">Go Back</a>





<script>
    function goBack() {
        window.history.back();
    }
</script>
